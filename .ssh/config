NoHostAuthenticationForLocalhost yes

# Forwarding
Host dionysus governator ssh pleiades maia columbia lou cfe1 cfe2 cfe3 lou1 lou2 lou3 bassi jacquard jen armstrong hecate artemis orangena thalia clio
  ForwardAgent yes
  ForwardX11 yes
  ForwardX11Trusted yes

# Dealing with firewalls
Host governator
  HostName localhost
  Port 8022

Host clio
  HostName localhost
  Port 2022

Host hecate
  HostName localhost
  Port 3022

# Princeton Machines
Host artemis hecate orangena
  User gnovak

# Nasa Machines
Host columbia lou cfe1 cfe2 cfe3 lou1 lou2 lou3 pleiadesfe1 pfe
  User gnovak

Host cfe1 cfe2 cfe3 lou1 lou2 lou3 pleiadesfe1
  ProxyCommand ssh -p 24 gnovak@sfe2.nas.nasa.gov /usr/local/bin/ssh-proxy %h

