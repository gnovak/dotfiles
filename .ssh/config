NoHostAuthenticationForLocalhost yes

# Forwarding
Host dionysus governatorpleiades maia columbia lou cfe1 cfe2 cfe3 lou1 lou2 lou3 jen bridge1 bridge2 armstrong sesame1 sesame2 thalia cliotigressdata pfe pfe-last blacklight
  ForwardAgent yes
  ForwardX11 yes
  ForwardX11Trusted yes

# Sometimes laptop's name isn't correctly set
Host thalia
  HostName localhost

# Dealing with firewalls
Host governator
  HostName localhost
  Port 1022

Host tigressdata
  HostName localhost
  Port 2022

Host sesame1
  HostName localhost
  Port 3022

# Mystery new machine: 4022

Host aramis
 HostName localhost
 Port 5022

